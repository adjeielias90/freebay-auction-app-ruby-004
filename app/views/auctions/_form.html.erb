<%= form_for @auction, html: {multipart: true} do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>
  <div>
    <%= f.label :end_time, "End Date" %>
    <%= f.datetime_field :end_time, :default => Date.today %>
  </div>
  <div>
  <%= f.label :picture, "Image" %>
    <%= f.file_field :picture %>
  </div>
  <div>
    <%= f.submit %>
  </div>

<% end %>